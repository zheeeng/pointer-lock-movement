{
  "name": "pointer-lock-movement",
  "version": "0.0.2",
  "author": "Zheeeng <hi@zheeeng.me>",
  "description": "A pointer lock movement manager for customizing your own creative UI.",
  "keywords": [
    "Pointer Lock API",
    "Mouse Lock API",
    "pointer capture",
    "mouse capture",
    "movement",
